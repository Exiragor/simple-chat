  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "browser-sync": "^2.23.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "extract-loader": "^2.0.1",
